getUsers:
SELECT
    username,
    password,
    display_name,
    email,
    coin_balance,
    online_status,
    register_timestamp,
    profile_pic_id
FROM user;

getExperts:
SELECT
    u.username,
    password,
    display_name,
    email,
    coin_balance,
    online_status,
    register_timestamp,
    profile_pic_id
FROM expert e INNER JOIN user u ON e.username = u.username;